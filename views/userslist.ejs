<%- include ('partials/navbar') %>
<%- include ('partials/messages') %>
<% if(typeof (userdata) != "undefined"){ %>
<div class="table-responsive">
    <table class="table table-striped table-hover table-dark">
        <thead>
            <tr class="bg-info">
                <th>SNO</th>
                <th>Name</th>
                <th>Email</th>
                <th>Password</th>
                <th>Imported At</th>

            </tr>
        </thead>
        <tbody>

            <%if (userdata.length >0){ %>
            <% userdata.forEach(function(row,index){ %>
            <tr>
                <td><%= index +1 %></td>
                <td><%= row.name %></td>
                <td><%= row.email %></td>
                <td><%= row.password %></td>
                <td><%= row.date %></td>
            </tr>


            <% })}else{ %>
            <tr>
                <td colspan="6">No Record Found</td>

            </tr>
            <% } %>
            <button style="margin-top:20px; margin-bottom:10px;" type="button" class="btn btn-outline-danger">
                <span class="badge badge-info"> <%=userdata.length %></span>Users Available
            </button>
        </tbody>
    </table>
</div>
<% } %>